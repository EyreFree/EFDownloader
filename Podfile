source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

platform :osx, '10.13'

use_frameworks!

# 忽略警告
inhibit_all_warnings!

target 'EFDownloader' do
    pod 'Alamofire', '4.7.1'                # 网络
    pod 'SDWebImage', '4.4.2'               # 图片
    pod 'SwiftyJSON', '4.2.0'               # JSON
end
